<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.shape.Polygon?>

<AnchorPane prefHeight="500" prefWidth="300" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
    <Pane>
        <!-- Background -->
        <Rectangle fill="rgb(255,0,255)" height="500" width="300" x="0" y="0" />
        <!-- Top Box -->
        <Rectangle fill="rgb(128,0,255)" height="150" width="290" x="0" y="0" />
        <!-- Bottom Box -->
        <Rectangle fill="rgb(128,0,255)" height="300" width="290" x="0" y="200" />

        <Polygon fill="rgb(64,0,64)" points="100.0, 100.0, 100.0, 100.0, 0.0, 100.0" />

        <!-- Triangle -->
        <Polygon fill="rgb(64,0,64)" layoutX="240.0" layoutY="70.0" rotate="180.0" stroke="WHITE" strokeWidth="5">
            <points>
                <Double fx:value="-50.0" />
                <Double fx:value="40.0" />
                <Double fx:value="50.0" />
                <Double fx:value="40.0" />
                <Double fx:value="0.0" />
                <Double fx:value="-60.0" />
            </points>
        </Polygon>

        <!-- Square -->
        <Rectangle fill="rgb(64,0,64)" x="200" y="405" height="90" width="90" stroke="WHITE" strokeWidth="5"></Rectangle>

        <!-- Player -->
        <Rectangle fill="rgb(0,255,255)" x="50" y="300" height="90" width="90" rotate="45" stroke="BLACK" strokeWidth="5"></Rectangle>
        <!-- LeftEye -->
        <Rectangle fill="rgb(0,255,64)" x="60" y="340" height="20" width="20" rotate="45" stroke="BLACK" strokeWidth="5"></Rectangle>
        <!-- RightEye -->
        <Rectangle fill="rgb(0,255,64)" x="87" y="310" height="20" width="20" rotate="45" stroke="BLACK" strokeWidth="5"></Rectangle>
        <!-- Mouth -->
        <Rectangle fill="rgb(0,255,64)" x="75" y="350" height="20" width="65" rotate="-45" stroke="BLACK" strokeWidth="5"></Rectangle>

    </Pane>
</AnchorPane>